
    <!DOCTYPE html>
    <html lang="en">
    <head>
      <meta charset="UTF-8">
      <meta http-equiv="X-UA-Compatible" content="IE=edge">
      <meta name="viewport" content="width=device-width, initial-scale=1.0">
      <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">

      <title>App en ejs</title>
    </head>
    <body>
      
      <a href="/" class="btn btn-primary m-4">Agregar un nuevo producto </a>
      <div class="d-flex justify-content-center flex-wrap">
   

      
        <% datos.forEach(function(producto) { %>
          <div class="card m-4" style="width: 18rem;">
          <img src=<%= producto.Thumbnail %> class="card-img-top" >
          <div class="card-body">
            <h5 class="card-title">Titulo <%= producto.Title %></h5>
            <h5 class="card-title">Precio $ <%= producto.Price %></h5>
            <h5 class="card-title">Id <%= producto.id %></h5>
            <a href="/producto/delete/<%= producto.id %>" class="btn btn-warning m-4">Eliminar </a>
          </div>
          </div>

          <% }); %>

    
      </div>
      
    </body>
    </html>
    



